@prefix ex:   <http://example.org/graph#> .
@prefix rel:  <http://example.org/relation#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

ex:Alice a foaf:Person ;
    foaf:name "Alice" ;
    ex:role "Engineer" .

ex:Bob a foaf:Person ;
    foaf:name "Bob" ;
    ex:role "Designer" .

ex:Carol a foaf:Person ;
    foaf:name "Carol" ;
    ex:role "Manager" .

ex:Dave a foaf:Person ;
    foaf:name "Dave" ;
    ex:role "Engineer" .

ex:TechCorp a foaf:Organization ;
    foaf:name "TechCorp" .

ex:Alice  rel:knows      ex:Bob .
ex:Alice  rel:knows      ex:Dave .
ex:Carol  rel:manages    ex:Alice .
ex:Carol  rel:manages    ex:Bob .
ex:Alice  rel:works_for  ex:TechCorp .
ex:Bob    rel:works_for  ex:TechCorp .

